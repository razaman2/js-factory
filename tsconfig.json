{
    "compilerOptions": {
        "target": "ES2017",
        "module": "CommonJS",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noUncheckedIndexedAccess": true,
        "resolveJsonModule": true,
        "noEmit": true
    }
}
